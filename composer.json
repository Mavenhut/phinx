{
    "name": "Mavenhut/phinx",
    "type": "library",
    "description": "Migration tool forked from robmorgan phinx",
    "keywords": ["migrations", "database", "db", "database migrations"],
    "license": "MIT",
    "authors": [{
        "name": "Rob Morgan",
        "email": "robbym@gmail.com",
        "homepage": "http://robmorgan.id.au",
        "role": "Lead Developer"
    }, {
        "name": "Woody Gilk",
        "email": "woody.gilk@gmail.com",
        "homepage": "http://shadowhand.me",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "~2.6",
        "symfony/config": "~2.6",
        "symfony/yaml": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "dev-phpcs-fixer"
    },
    "autoload": {
        "psr-4": {
            "Phinx\\": "src/Phinx"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Phinx\\": "tests/Phinx"
        }
    },
    "bin": ["bin/phinx"]
